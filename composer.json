{
    "name": "company4-dev/file-vault",
    "description": "",
    "keywords": [
        "laravel", "encrypt", "decrypt", "encryption", "decryption",
        "cbc", "php", "file", "file-vault"
    ],
    "homepage": "https://github.com/company4-dev/file-vault",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Costin Soare",
            "role": "Original Developer",
            "homepage": "https://github.com/soarecostin"
        },
        {
            "name": "Sam Carr√©",
            "role": "Previous Developer",
            "homepage": "https://github.com/Sammyjo20"
        },
        {
            "name": "Jason Millis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Company4\\FileVault\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "test-coverage": "./vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Company4\\FileVault\\FileVaultServiceProvider"
            ],
            "aliases": {
                "FileVault": "Company4\\FileVault\\Facades\\FileVault"
            }
        }
    }
}
